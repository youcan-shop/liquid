{
    "name": "youcanshop/liquid",
    "type": "library",
    "description": "Liquid template engine for themes.",
    "keywords": [
        "liquid",
        "template",
        "youcan"
    ],
    "homepage": "https://github.com/youcan-shop/liquid",
    "license": "MIT",
    "authors": [
        {
            "name": "Aymane D. Hlamnach",
            "email": "aymane.hlamnach@youcan.shop",
            "homepage": "https://github.com/azuradara"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.2"
    },
    "require-dev": {
        "ergebnis/composer-normalize": ">=2.8",
        "php-coveralls/php-coveralls": "^2.2",
        "phpunit/phpunit": "^9.2.6"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "YouCan\\Liquid\\": "src/Liquid"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YouCan\\Liquid\\": "tests/Liquid"
        }
    }
}
